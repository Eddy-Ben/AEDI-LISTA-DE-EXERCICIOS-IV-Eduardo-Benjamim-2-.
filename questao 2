#include <stdio.h>

int main() {
    int numero, i;

  
    printf("Por favor, insira um número inteiro positivo: ");
    scanf("%d", &numero);

    if (numero <= 0) {
        printf("Número inválido. Por favor, insira um número inteiro positivo.\n");
        return 1;
    }

    printf("Os números pares entre 1 e %d são:\n", numero);
    for (i = 2; i <= numero; i += 2) {
        printf("%d ", i);
    }
    printf("\n");

    return 0;
}
